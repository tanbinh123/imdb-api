                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/server.httpError"
                        }
                    }
                }
            }
        },
        "/title/{id}/photos": {
            "get": {
                "tags": [
                    "Title"
                ],
                "summary": "Titles photos",
                "parameters": [
                    {
                        "type": "string",
