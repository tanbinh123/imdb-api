                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "format": "integer",
                        "default": "1",
                        "description": "Page Number",
                        "name": "page",
                        "in": "query",
                        "required": true
                    }
                ],
